  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "TemplateContext": "Host=<host_id>;Database=<postgres_database>;Username=<postgres_user>;Password=<postgres_user_password>;"
  },
  "commandPath": "/bin/bash",
  "AdsumoModule": false,
  "Logs": false,
  "Tool": "TemplateApi",
  "LogAndAuth": {
    "Endpoint": "http://localhost:3000",
    "Token": "auth/token",
    "Info": "log/info",
    "Warn": "log/warn",
    "Error": "log/error"
  }
}
